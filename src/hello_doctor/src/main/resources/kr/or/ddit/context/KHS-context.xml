<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


<!-- Repository -->


<!-- DAO   -->
<bean id="case_DAO" class="kr.or.ddit.dao.Case_DAOImpl"
p:session-ref="sqlSession"/>
<bean id="replyDAO" class="kr.or.ddit.dao.CaseReplyDAOImpl"
p:session-ref="sqlSession"/>
<bean id="managerDAO" class="kr.or.ddit.dao.ManagerDAOImpl"
p:session-ref="sqlSession"/>
<bean id="noticeDAO" class="kr.or.ddit.dao.NoticeDAOImpl"
p:session-ref="sqlSession" />
<!-- Service -->
<bean id="case_Service" class="kr.or.ddit.service.CaseServiceImpl"
	p:casedao-ref="case_DAO"
/>
<bean id="replyService" class="kr.or.ddit.service.CaseReplyServiceImpl"
	p:replydao-ref="replyDAO"
/>
<bean id="managerService" class="kr.or.ddit.service.ManagerServiceImpl"
	p:managerdao-ref="managerDAO"
/>
<bean id="noticeService" class="kr.or.ddit.service.NoticeServiceImpl"
	p:noticedao-ref="noticeDAO"
/>
</beans>
